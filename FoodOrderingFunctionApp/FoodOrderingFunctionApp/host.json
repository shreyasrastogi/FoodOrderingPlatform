{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Information",
      "Function": "Information"
    }
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    },
    "openApi": {
      "enabled": true,
      "includeRequestingHostName": true,
      "forceHttp": false,
      "documentVersion": "3.0.1",
      "documentTitle": "Your API Title",
      "documentDescription": "API for managing pizza orders",
      "documentContactName": "Shreyas Rastogi",
      "documentContactEmail": "shreyasrastogi@gmail.com",
      "documentLicenseName": "MIT",
      "documentLicenseUrl": "https://opensource.org/licenses/MIT"
    }
  }
}